{
  "name": "reelforge-editor",
  "private": true,
  "version": "1.0.0-preview",
  "type": "module",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:electron": "vite --mode electron",
    "dev:watch": "nodemon --exec 'npm run dev' --watch vite.config.ts --watch package.json",
    "dev:stable": "while true; do npm run dev || sleep 3; done",
    "build": "tsc -b && vite build",
    "build:electron": "tsc -b && vite build --mode electron && electron-builder",
    "build:wasm": "cd dsp-wasm && wasm-pack build --target web --out-dir ../public/wasm && cd ..",
    "build:wasm:release": "cd dsp-wasm && wasm-pack build --target web --release --out-dir ../public/wasm && cd ..",
    "build:all": "npm run build:wasm && npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@pixi/react": "^8.0.5",
    "@tauri-apps/api": "^2.9.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "electron-store": "^11.0.2",
    "gsap": "^3.14.2",
    "jotai": "^2.16.1",
    "pixi.js": "^8.14.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.3.4",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.10.4",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.0.1",
    "nodemon": "^3.1.11",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.3.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^4.0.16"
  }
}
